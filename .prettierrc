{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "never",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "importOrder": [
    "^react",
    "^@?react([-\\w]*)?",
    "^next",
    "^@api",
    "^@store",
    "^@config",
    "^@layout",
    "^@modules",
    "^@module",
    "^@pages",
    "^@data",
    "^@components",
    "^@styles",
    "^@images",
    "^@heroicons",
    "^@?\\w",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}